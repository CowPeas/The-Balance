{
  "rewrites": [
    {
      "source": "/studio/:path*",
      "destination": "/studio/index.html"
    },
    {
      "source": "/work",
      "destination": "/work"
    },
    {
      "source": "/life",
      "destination": "/life"
    },
    {
      "source": "/business",
      "destination": "/business"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
